{ 
    "extends": ["react-native-wcandillon", "prettier"],
    "plugins": ["prettier"],
    "rules": {
      "import/no-default-export": "off",
      "arrow-body-style": "off",
      "react-hooks/exhaustive-deps": "off",
      "prefer-arrow-callback": "off",
      "@typescript-eslint/no-explicit-any": "off",
      "prettier/prettier": [
        "error",
        {
          "endOfLine": "auto"
        }
      ]
    } 
  }